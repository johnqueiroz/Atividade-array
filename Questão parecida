package programastestes;

import java.util.Scanner;
import java.util.Arrays;


public class QuestaoParecida {
	/**
	 * classe usada para pegar sequencia 10 numeros para depois ordenar e imprimi-los;
	 */
	int[] array = new int[10];
	Scanner ler = new Scanner(System.in);
	
	public void preencherLista() {
		/**
		 * metodo para preencher o array
		 */
		for(int i=0;i<10;i++) {
			System.out.println("digite o " + i +  "Â° do array");
			array[i]=ler.nextInt();
		}
}
	public void ordenar() {	
		/**
		 * metodo que ordana para imprimir o array
		 */
		
		//usado para ordenar o array de numeros inteiros;
		Arrays.sort(array);
		
		for (int i = 0; i<10; i++) {
			System.out.print(" " +array[i] );
		}		
		}
		
}
